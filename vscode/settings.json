{
	"python.formatting.provider": "black",
	"workbench.colorTheme": "GitHub Dark Default",
	"vim.incsearch": true,
	"vim.useSystemClipboard": true,
	"vim.useCtrlKeys": true,
	"vim.hlsearch": true,
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<leader>",
				"w"
			],
			"commands": [
				"workbench.action.files.save"
			]
		}
	],
	"vim.leader": "<space>",
	"redhat.telemetry.enabled": false,
	"git.confirmSync": false,
	"git.autofetch": true,
	"explorer.confirmDragAndDrop": false,
	"explorer.confirmDelete": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"editor.formatOnSave": true,
	"svelte.enable-ts-plugin": true,
	"explorer.confirmDelete": false,
	"python.venvPath": "~/.venv",
	"svelte.plugin.svelte.note-new-transformation": false,
	"rust-analyzer.inlayHints.renderColons": false,
	"rust-analyzer.inlayHints.chainingHints.enable": false,
	"rust-analyzer.inlayHints.closingBraceHints.enable": false,
	"rust-analyzer.inlayHints.parameterHints.enable": false,
	"rust-analyzer.inlayHints.typeHints.enable": false,
	"debug.allowBreakpointsEverywhere": true,
	"vs-kubernetes": {
		"vs-kubernetes.minikube-show-information-expiration": "2023-10-09T10:37:25.585Z"
	},
	"[python]": {
		"editor.formatOnType": true
	},
	"editor.lineNumbers": "relative",
	"refactai.apiKey": "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",
	"refactai.infurl": "https://code-scratchpads.smallcloud.ai/",
	"refactai.model": "bigcode/starcoder",
	"refactai.pauseCompletion": false,
	"llm.temperature": 0.2,
	"llm.contextWindow": 8192,
	"llm.tokensToClear": [
		"<|endoftext|>"
	],
	"llm.fillInTheMiddle.middle": "<fim_middle>",
	"llm.fillInTheMiddle.prefix": "<fim_prefix>",
	"llm.fillInTheMiddle.suffix": "<fim_suffix>",
	"llm.fillInTheMiddle.enabled": true,
	"llm.lsp.logLevel": "info",
	"llm.modelIdOrEndpoint": "http://localhost:4242",
	"llm.tokenizer": {
		"repository": "bigcode/starcoder"
	},
	"github.copilot.enable": {
		"*": true,
		"plaintext": false,
		"markdown": true,
